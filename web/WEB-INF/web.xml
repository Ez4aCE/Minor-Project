<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorkRegister</servlet-name>
        <servlet-class>MinorProjectBackend.WorkRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sessserve</servlet-name>
        <servlet-class>MinorProjectBackend.sessserve</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessServe</servlet-name>
        <servlet-class>MinorProjectBackend.SessServe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateEmail</servlet-name>
        <servlet-class>ValidateEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateEmails</servlet-name>
        <servlet-class>MinorProjectBackend.ValidateEmails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TrainDetails</servlet-name>
        <servlet-class>MinorProjectBackend.TrainDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessLogOut</servlet-name>
        <servlet-class>MinorProjectBackend.SessLogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogOut</servlet-name>
        <servlet-class>MinorProjectBackend.AdminLogOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SessLogOut</servlet-name>
        <url-pattern>/SessLogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorkRegister</servlet-name>
        <url-pattern>/WorkRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sessserve</servlet-name>
        <url-pattern>/sessserve</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessServe</servlet-name>
        <url-pattern>/SessServe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateEmail</servlet-name>
        <url-pattern>/ValidateEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateEmails</servlet-name>
        <url-pattern>/ValidateEmails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TrainDetails</servlet-name>
        <url-pattern>/TrainDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogOut</servlet-name>
        <url-pattern>/AdminLogOut</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
